<main class="contenedor contenedor-formularios">
  <h1><%= nombrePagina %></h1>
  <form method="POST" class="default-form" novalidate>
    <div class="campo">
      <label for="email">Correo</label>
      <input type="email" name="email" placeholder="correo@correo.com">
    </div>
    <div class="campo">
      <label for="nombre">Nombre</label>
      <input type="text" name="nombre" placeholder="John Doe">
    </div>
    <div class="campo">
      <label for="password">Contraseña</label>
      <input type="password" name="password" placeholder="********">
    </div>
    <div class="campo">
      <label for="confirmar">Repite tu Contraseña</label>
      <input type="password" name="confirmar" placeholder="********">
    </div>
    <div class="campo enviar">
      <input type="submit" class="btn btn-rosa" value="Crear Cuenta">
    </div>
  </form>
</main>